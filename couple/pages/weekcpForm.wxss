@font-face {
    font-family: 'fontcss';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2');
}

/* common */
upPhoto,button{
  font-family: 'fontcss';
}
page {
  font-family: 'fontcss';
  background: linear-gradient(90deg, rgba(255,255,255,.5) 8%,rgba(245,185,184,.5) 0),
              linear-gradient(rgba(255,255,255,.5) 8%,#FFB9B9 0);
               background-size: 30px 30px;
}

.fl-r {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.fl-b {
  align-items: center;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.wzjz {
  display: flex;
  align-items: center;
  justify-content: center;
}

/* Container */

.Container {
  position: relative;
  padding-bottom: 60rpx;
  box-sizing: border-box;
  width: 632rpx;
  margin: 0 auto;
  background: white;
  border-left: solid 6rpx #000;
  border-right: solid 6rpx #000;
  border-bottom: solid 6rpx #000;
}

/* delta */

.delta {
  position: relative;
  margin: 116rpx auto 0;
  width: 0;
  height: 0;
  border-right: solid 318rpx transparent;
  border-left: solid 318rpx transparent;
  /* 黑色大三角形 */
  border-bottom: solid 121rpx #000;
}

.delta::after {
  position: absolute;
  content: "";
  left: -306rpx;
  top: 6rpx;
  width: 0;
  height: 0;
  border-right: solid 306rpx transparent;
  border-left: solid 306rpx transparent;
  /* 白色小三角形 */
  border-bottom: solid 115rpx #fff;
}

/* header two */

.header view {
  width: 50%;
}

/* decorate*/

.decorate {
  position: relative;
  width: 520rpx;
  height: 16rpx;
  margin: 0 auto;
  border-top: solid #000 6rpx;
  border-bottom: solid #000 6rpx;
}

.decorate image {
  position: absolute;
  left: 50%;
  top: -50%;
  transform: translate(-50%, -30%);
  width: 126rpx;
  height: 104rpx;
}

/* title */

.title {
  position: relative;
  height: 220rpx;
  margin: 80rpx auto 20rpx;
}

.title image {
  display: block;
  width: 417rpx;
  height: 220rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.title .pxsbx {
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 9;
  width: 100rpx;
  height: 200rpx;
  background: #fff;
  transform: translate(-50%, -50%) rotate(-90deg) skew(15deg);
  border: #000 solid 5rpx;
}

.title .pxsbx view {
  /* line-hight=div height实现文字垂直居中 */
  line-height: 100rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  text-align: center;
  width: 200rpx;
  height: 100rpx;
  z-index: 99;
  transform: translate(-50%, -50%) rotate(90deg) skew(15deg);
}

/* form */

.wrap, .phone-wrap {
  margin: 40rpx auto;
  width: 80%;
}

.wrap input, .phone-wrap input, .wrap textarea {
  box-sizing: border-box;
  margin-top: 15rpx;
  height: 75rpx;
  width: 100%;
  background: #fceef0;
  border: 5rpx solid #000;
  border-radius: 50rpx;
  padding: 0 25rpx;
}

button {
  margin-top: 15rpx;
  box-sizing: border-box;
  height: 75rpx;
  border: 5rpx solid #000;
  width: 50%;
  float: left;
  line-height: 75rpx;
  color: gray;
}

/* upLoad photo */

button.up {
  border-radius: 50rpx;
}

/* check sex */
.wrap label{
  font-family: 'fontcss';
  display: block;
  width: 100%;
}
button.left,button.right{
  width: 40%;
  border-radius: 50rpx;
}
button.left{
  float: left;
}
button.right{
  float: right;
}
.wrap .clear{
  clear: both;
}

/* phone */

.phone-wrap input, .phone-wrap button {
  height: 75rpx;
  box-sizing: border-box;
  border: 5rpx solid #000;
}

.phone-wrap label {
  font-family: 'fontcss';
  display: block;
  margin-bottom: 15rpx;
}

.phone-wrap input {
  margin: 0;
  width: 60%;
}

.phone-wrap button {
  border-radius: 50rpx;
  line-height: 75rpx;
  font-size: 25rpx;
  width: 35%;
  margin: 0;
}

/* textarea */

.wrap textarea {
  padding: 20rpx;
  border-radius: 20rpx;
  height: 200rpx;
}

/* submit button */

.submit-wrap {
  position: relative;
  width: 260rpx;
  height: 110rpx;
  left: 50%;
  transform: translate(-50%);
}

.submit-wrap button, .submit-wrap image {
  width: 100%;
  height: 100%;
  display: block;
  position: absolute;
  top: 0;
  left: 0;
}

.submit-wrap button {
  border: none;
  background-color: transparent;
}

button::after {
  border: none;
}

/* cpImg */

.cpImg {
  display: block;
  margin: 20rpx auto 0;
  width: 173rpx;
  height: 62rpx;
}

/* hinder */

.hinder, .cover {
  box-sizing: border-box;
  width: 676rpx;
  margin: 0 auto;
  position: relative;
  border: 6rpx solid #000;
}
.hinder { /*在后面被遮住*/
  height: 356rpx;
  z-index: -1;
  background: #efd8d8;
  top: -164rpx; /* moveUp */ 

}

.cover {
  margin-bottom: -356rpx; /*等高去占的空间*/
  top:-482rpx; /*356 + 164 */
  z-index: 1;
  width: 0;
  height: 0;
  border-top: solid transparent 178rpx;
  border-left:solid   #000 338rpx; 
  border-right:solid   #000 338rpx;
  border-bottom:solid   #000 178rpx;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}
.cover::after{
  position: absolute;
  content: "";
  top:-170rpx; /*178-6*/
  left:-332rpx; /*676/2-6*/
  width: 0;
  height: 0;
  border-top: solid transparent 178rpx;
  border-left:solid  #ee5d7e  332rpx;/*676/2-6*/
  border-right:solid   #ee5d7e 332rpx;
  border-bottom:solid   #ee5d7e 166rpx;/*676/2-6-6*/
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}
/* logo */
.logo{
  width: 159rpx;
  height: 176rpx;
  display: block;
  margin: 0 auto;
  position: relative;
  top:-100rpx;
}
/* window */
.window{
  z-index: 999;
  position: fixed;
  top:0;
  left: 0;
  width: 100vw;height: 100vh;
  background-color: rgba(0,0,0,.5);
  
  
}
.window .block{
  position: fixed;
  left: 0;right: 0;top: 0;bottom: 0;margin:auto;
  width: 500rpx;
  height: 800rpx;
  border-radius: 50rpx;
  background: white;
}
.window .block .two-btn{
  position: absolute;
  bottom: 20rpx;
  width: 80%;
  left: 50%;
  transform: translate(-50%);
}
.window .block .two-btn button{
  width: 40%;
  margin: 5%;
  
}
.window .block .photo{
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  bottom: 200rpx;
  width: 80%;
  height: 60%;
  background: pink;
}
.foot{
  position: relative;
  top: -100rpx;
  color: white;
  font-size: 25rpx;
  font-family: 'Arial'
}