<!--pages/user/user.wxml-->
<view>
  <view  class="userinfo">
    <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
    <text class="userinfo-nickname">{{userInfo.nickName}}</text>
  </view>
  <view class="usermotto">
    
    <navigator class="list_a" url="publish?accessToken={{accessToken}}">
        我发布的
    </navigator>
    <view class="list_b" style="position:relative;">
        联系我们
        <button class="btn" style="position:absolute;top:0;bottom:0;width:100%;height:100%;z-index:100" plain="true" open-type="contact"></button>
    </view>
  </view>

</view>
